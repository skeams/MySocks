<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#266d91"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/SUMAmount"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="none"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/SUMDesc"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <TextView
        android:id="@+id/SUMDesc"
        android:layout_width="109dp"
        android:layout_height="50dp"
        android:text="Sum"
        android:textColor="#CECECE"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <EditText
        android:id="@+id/DrikkeAmount"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="none"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/DrikkeDesc"
        app:layout_constraintTop_toTopOf="@+id/guideline" />

    <EditText
        android:id="@+id/AnnetJuster"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="number"
        android:selectAllOnFocus="true"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/AnnetAdd"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toBottomOf="@+id/AnnetAmount" />

    <Button
        android:id="@+id/AnnetAdd"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="+"
        android:textColor="#8BFF94"
        android:textSize="36sp"
        app:layout_constraintEnd_toStartOf="@+id/AnnetSub"
        app:layout_constraintTop_toBottomOf="@+id/AnnetAmount" />

    <Button
        android:id="@+id/AnnetSub"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="-"
        android:textColor="#FF7167"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/AnnetAmount" />

    <EditText
        android:id="@+id/UteJuster"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="number"
        android:selectAllOnFocus="true"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/UteAdd"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toBottomOf="@+id/UteAmount" />

    <Button
        android:id="@+id/UteAdd"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="+"
        android:textColor="#8BFF94"
        android:textSize="36sp"
        app:layout_constraintEnd_toStartOf="@+id/UteSub"
        app:layout_constraintTop_toBottomOf="@+id/UteAmount" />

    <Button
        android:id="@+id/UteSub"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="-"
        android:textColor="#FF7167"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/UteAmount" />

    <EditText
        android:id="@+id/DrikkeJuster"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="number"
        android:selectAllOnFocus="true"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/DrikkeAdd"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toBottomOf="@+id/DrikkeAmount" />

    <Button
        android:id="@+id/DrikkeAdd"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="+"
        android:textColor="#8BFF94"
        android:textSize="36sp"
        app:layout_constraintEnd_toStartOf="@+id/DrikkeSub"
        app:layout_constraintTop_toBottomOf="@+id/DrikkeAmount" />

    <Button
        android:id="@+id/DrikkeSub"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="-"
        android:textColor="#FF7167"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/DrikkeAmount" />

    <Button
        android:id="@+id/MatSub"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="-"
        android:textColor="#FF7167"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/MatAmount" />

    <EditText
        android:id="@+id/AnnetAmount"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="none"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/AnnetDesc"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <EditText
        android:id="@+id/UteAmount"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="none"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/UteDesc"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <TextView
        android:id="@+id/AnnetDesc"
        android:layout_width="109dp"
        android:layout_height="50dp"
        android:text="Annet"
        android:textColor="#FF74A5"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <TextView
        android:id="@+id/UteDesc"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="Ute"
        android:textColor="#74CCFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <TextView
        android:id="@+id/DrikkeDesc"
        android:layout_width="103dp"
        android:layout_height="50dp"
        android:text="Drikke"
        android:textColor="#FFD174"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.24" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.46" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68" />

    <TextView
        android:id="@+id/MatDesc"
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="Mat"
        android:textColor="#74FF79"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline9" />

    <EditText
        android:id="@+id/MatAmount"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="none"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/MatDesc"
        app:layout_constraintTop_toTopOf="@+id/guideline9" />

    <EditText
        android:id="@+id/MatJuster"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="number"
        android:selectAllOnFocus="true"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/MatAdd"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toBottomOf="@+id/MatDesc" />

    <Button
        android:id="@+id/MatAdd"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:ellipsize="middle"
        android:paddingTop="0dp"
        android:text="+"
        android:textColor="#8BFF94"
        android:textSize="36sp"
        app:layout_constraintEnd_toStartOf="@+id/MatSub"
        app:layout_constraintTop_toBottomOf="@+id/MatAmount" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="16dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02" />

</androidx.constraintlayout.widget.ConstraintLayout>